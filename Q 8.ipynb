    "#8. Reverse a vector (first element becomes last)\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "# Create a sample vector\n",
    "my_vector = np.array([1, 2, 3, 4, 5])\n",
    "\n",
    "# Reverse the vector\n",
    "reversed_vector = my_vector[::-1]\n",
    "\n",
    "print(reversed_vector)\n"
   ]
