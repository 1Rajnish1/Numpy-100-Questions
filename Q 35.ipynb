   "source": [
    "#35. How to compute ((A+B)*(-A/2)) in place (without copy)?\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "A = np.array([1, 2, 3])\n",
    "B = np.array([4, 5, 6])\n",
    "\n",
    "\n",
    "np.multiply((A + B),(-A / 2))\n",
    "\n",
    "\n",
    "print(A)\n"
   ]
