   "source": [
    "#10. Find indices of non-zero elements from [1,2,0,0,4,0]\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "# Create the array\n",
    "my_array = np.array([1, 2, 0, 0, 4, 0])\n",
    "\n",
    "# Find the indices of non-zero elements using boolean indexing\n",
    "indices = np.where(my_array != 0)\n",
    "\n",
    "print(indices)\n"
   ]
