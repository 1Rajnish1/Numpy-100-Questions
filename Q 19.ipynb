   "source": [
    "#19. Create a 8x8 matrix and fill it with a checkerboard pattern\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "matrix = np.zeros((8, 8), dtype=int)\n",
    "\n",
    "\n",
    "matrix[1::2, ::2] = 1\n",
    "matrix[::2, 1::2] = 1\n",
    "\n",
    "print(matrix)\n"
