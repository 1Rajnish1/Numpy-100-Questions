    "#45. Create random vector of size 10 and replace the maximum value by 0\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "random_vector = np.random.rand(10)\n",
    "\n",
    "max_index = np.argmax(random_vector)\n",
    "\n",
    "random_vector[max_index] = 0\n",
    "\n",
    "print(random_vector)"
